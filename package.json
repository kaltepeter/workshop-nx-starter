{
  "name": "tuskdesk-suite",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected apps",
    "affected:build": "./node_modules/.bin/nx affected build",
    "affected:e2e": "./node_modules/.bin/nx affected e2e",
    "format": "./node_modules/.bin/nx format write",
    "format:write": "./node_modules/.bin/nx format write",
    "format:check": "./node_modules/.bin/nx format check",
    "legacy-agent-workdesk": "live-server ./legacy/agent-workdesk/app --host=localhost --port=8000 --entry-file=index.html --quiet",
    "server": "tsc -p ./server && node dist/server/server.js",
    "server:throttled": "tsc -p ./server && node dist/server/server.js --throttle",
    "reporting": "ng serve --proxy-config=proxy.config.json -a=reporting -p=4202",
    "customer-portal": "ng serve --proxy-config=proxy.config.json -a=customer-portal -p=4203",
    "logs": "ng serve --proxy-config=proxy.config.json -a=logs -p=4204",
    "postinstall": "./node_modules/.bin/nx postinstall",
    "update": "./node_modules/.bin/nx update",
    "update:check": "./node_modules/.bin/nx update check",
    "update:skip": "./node_modules/.bin/nx update skip",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "affected:dep-graph": "./node_modules/.bin/nx affected dep-graph",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.7",
    "@angular/common": "5.2.7",
    "@angular/compiler": "5.2.7",
    "@angular/core": "5.2.7",
    "@angular/forms": "5.2.7",
    "@angular/http": "5.2.7",
    "@angular/platform-browser": "5.2.7",
    "@angular/platform-browser-dynamic": "5.2.7",
    "@angular/platform-server": "5.2.7",
    "@angular/router": "5.2.7",
    "@angular/service-worker": "5.2.7",
    "@ngrx/effects": "5.1.0",
    "@ngrx/entity": "4.1.1",
    "@ngrx/router-store": "5.0.1",
    "@ngrx/store": "5.1.0",
    "@ngrx/store-devtools": "5.1.0",
    "@nrwl/nx": "^0.11.1",
    "body-parser": "1.18.2",
    "core-js": "^2.4.1",
    "express": "4.16.2",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.7.1",
    "@angular/compiler-cli": "5.2.7",
    "@angular/language-service": "5.2.7",
    "@nrwl/schematics": "^0.11.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.5.1",
    "codelyzer": "4.0.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "live-server": "^1.2.0",
    "ngrx-store-freeze": "0.2.0",
    "prettier": "1.10.2",
    "protractor": "5.2.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.6.2"
  }
}
